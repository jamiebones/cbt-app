# Development Environment Variables
REACT_APP_API_URL=http://localhost:4000
REACT_APP_LOCAL_SERVER_URL=http://localhost:5000
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_CALCULATOR=true
REACT_APP_ENABLE_MEDIA_UPLOAD=true
REACT_APP_ENABLE_EXCEL_IMPORT=true

# File Upload Limits (in bytes)
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_MAX_IMAGE_SIZE=5242880
REACT_APP_MAX_AUDIO_SIZE=10485760

# Application Settings
REACT_APP_DEFAULT_TEST_TIME_LIMIT=60
REACT_APP_AUTO_SAVE_INTERVAL=30000

# Analytics (optional)
REACT_APP_GOOGLE_ANALYTICS_ID=
REACT_APP_HOTJAR_ID=
